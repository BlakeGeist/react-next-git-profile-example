{
  "name": "nextjs-with-firebase-hosting",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects.",
  "repository": "https://github.com/jthegedus/firebase-functions-next-example.git",
  "scripts": {
    "next": "npm run build-firebase && cd \"src/app\" && npm install",
    "preserve": "npm run build-all",
    "serve": "firebase serve",
    "predeploy": "npm run build-all",
    "deploy": "firebase deploy",
    "build-all": "npm run build-next && npm run build-firebase",
    "build-next": "cd \"src/app\" && npm install && npm run-script build",
    "build-firebase": "cd \"src/functions\" && npm install"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.6.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "core-js": "^2.6.4",
    "immutability-helper": "^3.0.1",
    "moment": "^2.24.0",
    "next": "9.0.5",
    "next-redux-wrapper": "^3.0.0",
    "next-transpile-modules": "^2.3.1",
    "react": "16.9.0",
    "react-dnd-cjs": "^9.3.9",
    "react-dnd-html5-backend-cjs": "^9.3.9",
    "react-dom": "16.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8"
  }
}
